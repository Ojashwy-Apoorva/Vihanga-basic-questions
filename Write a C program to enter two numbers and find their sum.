// Write a C program to enter two numbers and find their sum.
#include <stdio.h>
#include <conio.h>
int main() {
int a, b, c;
printf("Kindly write the first number:");
scanf("%d", &a);
printf("kindly write the second no: ");
scanf("%d", &b);
c = a + b;
printf("Addition of 1st and 2nd no is : %d", c);
    return 0;
}
